{
  "name": "pcre2-wasm-universal",
  "version": "1.0.0",
  "description": "Universal (Node.js + browser) fork of @stephen-riley/pcre2-wasm - PCRE2 (Perl Compatible Regular Expressions 2) compiled to WebAssembly",
  "type": "module",
  "main": "./src/PCRE.js",
  "module": "./src/PCRE.js",
  "exports": {
    ".": {
      "import": "./src/PCRE.js",
      "default": "./src/PCRE.js"
    },
    "./libpcre2": {
      "import": "./dist/libpcre2.js",
      "default": "./dist/libpcre2.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "pcre",
    "pcre2",
    "perl",
    "regex",
    "browser",
    "node",
    "wasm",
    "webassembly"
  ],
  "author": "Universal fork by grepr project, original by Stephen Riley <jstephen.riley@yahoo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephen-riley/pcre2-wasm.git"
  },
  "homepage": "https://github.com/stephen-riley/pcre2-wasm#readme",
  "bugs": {
    "url": "https://github.com/stephen-riley/pcre2-wasm/issues"
  }
}
